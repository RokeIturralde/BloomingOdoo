<odoo>
    <data>
        <!-- action opening views on models-->
        <record model="ir.actions.act_window" 
        id="blooming.challenge_action_window">
            <field name="name">Challenge Window</field>
            <field name="res_model">blooming.challenge</field>
            <field name="view_mode">tree, form</field>
        </record>

        <!-- actions-->
        <menuitem name="Challenge management"
        id="blooming.menu_4_list"
        parent="blooming_menu_1"
        action="blooming.membershipplan_action_window"/>


        <record>
            <field name="model">blooming.challenge</field>
            <field name="arch" type="xml">
                <tree string="Challenge list">
                    <field name="name"/>
                    <field name="challenge_id"/>
                    <field name="end_date"></field>
                    <field name="description"/>
                    <field name="creator"/>
                    <field name="participants">
                        <!-- -->
                    </field>
                </tree>
            </field>
        </record>



        <record model="ir.ui.view" id="blooming.form">
            <field name="name">blooming list</field>
            <field name="model">blooming.membershipplan</field>
            <field name="arch" type="xml">
                <form string="Membership plan form">
                    <group colspan="4">
                        <group colspan="2" col="2">
                            <separator string="General stuff" colspan="2"/>
                            <field name="name"/>
                            <field name="description"/>
                        </group>
                        <group colspan="2" col="2">
                            <field name="albumLimit" colspan="2"/>
                            <field name="duration"/>
                            <field name="price"/>
                        </group>
                        <separator string="Shareable" colspan="2"/>
                        <field name="shareable" nolabel="1"/>   
                        <separator string="Users" colspan="2"/>
                        <field name="user_ids"/>
                    </group>
                </form>
            </field>
        </record>

        
        

        

       


        <!--<record model="ir.ui.view" id="view_content_form">-->
            
       
    </data>
</odoo>